# Uncomment this line to define a global platform for your project
platform :ios, '8.0'

target 'DouDou' do
  # Uncomment this line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for DouDou
  pod 'AFNetworking'
  pod 'Masonry'
  pod 'MJExtension'
  pod 'FMDB'
  pod 'MBProgressHUD'
  pod 'YYKit'
  pod 'RongCloudIMKit'

  target 'DouDouTests' do
       inherit! :search_paths
    
    pod 'Kiwi'
    # Pods for testing
  end

end
